@model CvPreviewModel

@if (Model.Skills.Count != 0)
{
    <div class="divTableHeading">
        <div class="divTableRow">
            <div class="divTableHead">Languages</div>
            <div class="divTableHead"></div>
        </div>
    </div>
    <div class="divTableBody">
        @for (int i = 0; i < Model.Languages.Count; i++)
        {
            <div class="divTableRow">
                <div class="divTableCell"></div><div class="divTableCell">@Html.DisplayFor(model => model.Languages[i].Name) - @Html.DisplayFor(model => model.Languages[i].Level)</div>
            </div>
        }
    </div>
}
